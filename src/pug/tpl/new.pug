main.main
	header.container.main-wrapper
		.row
			.container-full.hyphenation
				h1
					|[*pagetitle*]
	article.container.main-wrapper
		.row
			.container-full.hyphenation.text-justify
				p.article-time
					time.icon-calendar(datetime="[*news_date*]")
						|[[aDate? &date=`[*news_date*]`]] года
				|[*content*]
				|<@IF:[*EvoVideo:isNotEmpty*]>
				.evovideo(style="margin-bottom: 1em;")
					|[[GetEvoVideo? &url=`[*EvoVideo*]`]]
				|<@ENDIF>
				|[[multiTV?
				|	&tvName=`photogallery`
				|	&docid=`[*id*]`
				|	&display=`all`
				|	&emptyOutput=`1`
				|]]
				p.article-autor
					strong
						|[*news_person*]
	|{{@FILE assets/templates/projectsoft/tpl/govlist.html}}